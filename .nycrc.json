{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/*.cy.ts",
    "**/*.d.ts",
    "src/test.ts",
    "src/polyfills.ts",
    "src/environments/**",
    "src/main.ts"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "lcov",
    "text-summary",
    "cobertura",
    "json"
  ],
  "report-dir": "coverage/cypress",
  "cache": false,
  "instrument": false,
  "sourceMap": false
}